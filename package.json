    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "laravel-mix": "^6.0.31",
    "laravel-mix-copy-watched": "^2.3.1",
    "npm-run-all": "^4.1",
    "postcss": "^8.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "sass-loader": "11.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {
    "@ky-is/tailwind-color-palette": "^1.0.0",
